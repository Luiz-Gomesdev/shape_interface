package br.com.luiz.shape.entitie;

import br.com.luiz.shape.enums.Color;

public class AbstractShape {
	
	private Color color;

	public AbstractShape(Color color) {
		this.color = color;
	}

	public Color getColor() {
		return color;
	}

	public void setColor(Color color) {
		this.color = color;
	}

}
